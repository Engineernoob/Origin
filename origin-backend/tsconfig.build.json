{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": false,
    "noImplicitAny": false,
    "strict": false
  },
  "exclude": [
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "node_modules"
  ]
}
